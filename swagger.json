{
  "openapi": "3.0.0",
  "info": {
    "title": "Travel Diary Platform Backend Development API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/api/v1",
      "description": "localhost"
    }
  ],
  "paths": {
    "/user/signup": {
      "post": {
        "tags": ["UsersNew"],
        "description": "Create a new user in the system",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Signup"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "New user is created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/login": {
      "post": {
        "tags": ["UsersNew"],
        "description": "User login",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User logged in successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/logout": {
      "post": {
        "tags": ["UsersNew"],
        "description": "User logout",
        "responses": {
          "200": {
            "description": "User logout in successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/forgetPassword": {
      "post": {
        "tags": ["UsersNew"],
        "description": "User forgetPassword",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User updated Password in successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/allUsers": {
      "get": {
        "tags": ["UsersNew"],
        "description": "get All Users in the system",
        "responses": {
          "201": {
            "description": "get All Users retrived",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/{id}": {
      "get": {
        "tags": ["UsersNew"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "get user By Id",
            "schema": {
              "$ref": "#/components/schemas/id"
            }
          }
        ],
        "description": "get All Users in the system",
        "responses": {
          "201": {
            "description": "get All Users retrived",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/update": {
      "put": {
        "tags": ["UsersNew"],
        "description": "User Update",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/updateUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/user/delete/{id}": {
      "delete": {
        "tags": ["UsersNew"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "delete user By Id",
            "schema": {
              "$ref": "#/components/schemas/id"
            }
          }
        ],
        "description": "delete Users in the system",
        "responses": {
          "201": {
            "description": "delete Users",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/diary/entries/create": {
      "post": {
        "tags": ["Diary Entries"],
        "summary": "Create a new Diary Entries post",
        "description": "Create a new Diary Entries post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateDiaryEntries"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Blog post created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/diary/entries/all/{userId}": {
      "get": {
        "tags": ["Diary Entries"],
        "parameters": [
          {
            "name": "userId",
            "in": "path",
            "description": "get Diary Entries By userId"
          }
        ],
        "summary": "Get all Diary Entries posts",
        "description": "Get all Diary Entries posts",
        "responses": {
          "200": {
            "description": "Diary Entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/diary/entries/get/{entryId}": {
      "get": {
        "tags": ["Diary Entries"],
        "summary": "Get a Diary Entries by ID",
        "description": "Get a Diary Entries by its ID",
        "parameters": [
          {
            "name": "entryId",
            "in": "path",
            "description": "Diary Entries By entryId",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Diary Entries retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/diary/entries/update": {
      "put": {
        "tags": ["Diary Entries"],
        "summary": "Update a Diary Entries",
        "description": "Update a Diary Entries",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateDiaryEntries"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Diary Entries updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/diary/entries/delete/{entryId}": {
      "delete": {
        "tags": ["Diary Entries"],
        "summary": "Delete a Diary Entries by ID",
        "description": "Delete a Diary Entries by its ID",
        "parameters": [
          {
            "name": "entryId",
            "in": "path",
            "description": "Diary Entries ID",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Diary Entries deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Signup": {
        "required": [
          "userName",
          "userEmail",
          "userPassword",
          "userFullName",
          "dateOfBirth",
          "profilePicture",
          "isAdmin"
        ],
        "properties": {
          "userName": {
            "type": "string",
            "default": "Satish"
          },
          "userEmail": {
            "type": "string",
            "default": "satish@gmail.com"
          },
          "userPassword": {
            "type": "string",
            "default": "satish@2024"
          },
          "userFullName": {
            "type": "string",
            "default": "Satish Thodeti"
          },
          "dateOfBirth": {
            "type": "string",
            "default": "2000-01-01"
          },
          "profilePicture": {
            "type": "string",
            "default": "https://images.pexels.com/photos/2108845/pexels-photo-2108845.jpeg?auto=compress&cs=tinysrgb&w=600"
          },
          "isAdmin": {
            "type": "boolean",
            "default": false
          }
        }
      },
      "login": {
        "required": ["userEmail", "userPassword"],
        "properties": {
          "userEmail": {
            "type": "string",
            "default": "satish@gmail.com"
          },
          "userPassword": {
            "type": "string",
            "default": "satish@2024"
          }
        }
      },
      "updateUser": {
        "required": [
          "tun",
          "userName",
          "userEmail",
          "userPassword",
          "userFullName",
          "dateOfBirth",
          "profilePicture",
          "isAdmin"
        ],
        "properties": {
          "userId": {
            "type": "integer",
            "default": 1
          },
          "tun": {
            "type": "string",
            "default": "TUN1"
          },
          "userName": {
            "type": "string",
            "default": "Satish"
          },
          "userEmail": {
            "type": "string",
            "default": "satish@gmail.com"
          },
          "userPassword": {
            "type": "string",
            "default": "satish@2024"
          },
          "userFullName": {
            "type": "string",
            "default": "Satish Thodeti"
          },
          "dateOfBirth": {
            "type": "string",
            "default": "2000-01-01"
          },
          "profilePicture": {
            "type": "string",
            "default": "https://images.pexels.com/photos/2108845/pexels-photo-2108845.jpeg?auto=compress&cs=tinysrgb&w=600"
          },
          "isAdmin": {
            "type": "boolean",
            "default": false
          }
        }
      },
      "id": {
        "required": ["id"],
        "properties": {
          "id": {
            "type": "integer",
            "default": 1
          }
        }
      },
      "CreateDiaryEntries": {
        "required": [
          "userId",
          "title",
          "description",
          "date",
          "location",
          "photos",
          "createdBy"
        ],
        "properties": {
          "userId": {
            "type": "integer",
            "default": 1,
            "description": "The ID of the user creating the diary entry."
          },
          "title": {
            "type": "string",
            "default": "Sample Title",
            "description": "The title of the diary entry."
          },
          "description": {
            "type": "string",
            "default": "This is the description.",
            "description": "The description of the diary entry."
          },
          "date": {
            "type": "string",
            "format": "date",
            "default": "2024-04-10",
            "description": "The date of the diary entry."
          },
          "location": {
            "type": "string",
            "default": "Sample Location",
            "description": "The location of the diary entry."
          },
          "photos": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": ["photo1.jpg", "photo2.jpg"],
            "description": "An array of photo URLs for the diary entry."
          }
        }
      },
      "UpdateDiaryEntries": {
        "required": [
          "entryId",
          "userId",
          "title",
          "description",
          "date",
          "location",
          "photos",
          "createdBy"
        ],
        "properties": {
          "entryId": {
            "type": "integer",
            "default": 1,
            "description": "The ID of the user creating the diary entry."
          },
          "userId": {
            "type": "integer",
            "default": 1,
            "description": "The ID of the user creating the diary entry."
          },
          "title": {
            "type": "string",
            "default": "Sample Title",
            "description": "The title of the diary entry."
          },
          "description": {
            "type": "string",
            "default": "This is the description.",
            "description": "The description of the diary entry."
          },
          "date": {
            "type": "string",
            "format": "date",
            "default": "2024-04-10",
            "description": "The date of the diary entry."
          },
          "location": {
            "type": "string",
            "default": "Sample Location",
            "description": "The location of the diary entry."
          },
          "photos": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": ["photo1.jpg", "photo2.jpg"],
            "description": "An array of photo URLs for the diary entry."
          }
        }
      },
      "Response": {
        "properties": {
          "message": {
            "type": "string"
          }
        }
      }
    }
  }
}
